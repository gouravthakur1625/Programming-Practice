    "def subarray(arr,k):\n",
    "    s =[]\n",
    "    n = len(arr)\n",
    "    for i in range(n):\n",
    "        s.append(arr[i])\n",
    "        while(sum(s)>k):\n",
    "            s.pop(0)\n",
    "        if(sum(s)==k):\n",
    "            return s\n",
    "    return []\n",
    "arr = [1,2,3,4,5,6,7]\n",
    "print(subarray(arr,))      "
   ]
